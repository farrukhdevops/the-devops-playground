{
  "title": "TDP â€” Containers",
  "uid": "tdp-containers",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    { "type": "timeseries", "title": "CPU usage (container)",
      "gridPos": {"x":0,"y":0,"w":12,"h":6},
      "targets": [ { "expr": "sum by (name) (rate(container_cpu_usage_seconds_total{image!=\"\",name!~\"/|^$\"}[2m]))",
                     "legendFormat": "{{name}}" } ] },
    { "type": "timeseries", "title": "Memory working set (container)",
      "gridPos": {"x":12,"y":0,"w":12,"h":6},
      "targets": [ { "expr": "container_memory_working_set_bytes{image!=\"\",name!~\"/|^$\"}",
                     "legendFormat": "{{name}}" } ] },
    { "type": "timeseries", "title": "Network IO (bytes/s)",
      "gridPos": {"x":0,"y":6,"w":24,"h":6},
      "targets": [
        { "expr": "sum by (name) (rate(container_network_receive_bytes_total{image!=\"\",name!~\"/|^$\"}[2m]))",
          "legendFormat": "{{name}} rx" },
        { "expr": "sum by (name) (rate(container_network_transmit_bytes_total{image!=\"\",name!~\"/|^$\"}[2m]))",
          "legendFormat": "{{name}} tx" }
      ] }
  ]
}
